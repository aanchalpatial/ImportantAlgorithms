 #include<iostream>
using namespace std;

int main()
{
   int arr[]={2,4,6,8,10};
   int num;
   cin>>num;
   int first,last,mid;
   first=0;
   last=4;
   bool flag=false;
   while(first<=last)
   {
       mid=(first+last)/2;
       if(num<mid)
       {
           last=mid-1;
       }
       else if(num >mid)
       {
           first=mid+1;
       }
       else if(num==mid)
       {   flag=true;
           break;
       }
   }
   if(flag)
   cout<<"found!!!";
   else
   cout<<"not found.";
   return 0;
}
